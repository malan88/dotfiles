[user]
	name = malan88
	email = m@stdwtr.io
[core]
	editor = nvim
	excludesfile = /home/malan/.gitinore_global
[alias]
	graph = log --oneline --decorate --graph --all
	s = status
	a = add
	c = commit
	co = checkout
    ec = config --global -e
	ctags = !.git/hooks/ctags
    dc = diff --cached 
    ds = diff --staged 
	ignore = "!gi() { curl -sL https://www.toptal.com/developers/gitignore/api/$@ ;}; gi"
[pull]
	rebase = true
[url "git@github.com:malan88/"]
    insteadOf = me:
[url "git@github.com:Anno-Wiki/"]
    insteadOf = icc:
[init]
	defaultBranch = main
